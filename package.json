{
  "name": "invoice-automation",
  "version": "0.1.0",
  "description": "Automated invoice downloader for freelancers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "invoice",
    "automation",
    "datev",
    "playwright"
  ],
  "dependencies": {
    "playwright": "^1.40.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "crypto-js": "^4.2.0",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/crypto-js": "^4.1.1",
    "@types/cors": "^2.8.15",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}